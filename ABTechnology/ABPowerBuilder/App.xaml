<Application x:Class="ABPowerBuilder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ABPowerBuilder"
             >
    <Application.Resources>

		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Resources/Defaults.xaml" />
				<ResourceDictionary Source="/Resources/DataTemp.xaml" />
				<ResourceDictionary Source="/Resources/ReStyle.xaml" />
				<ResourceDictionary Source="/Resources/UI.xaml" />
			</ResourceDictionary.MergedDictionaries>
			
		</ResourceDictionary>
		
         
    </Application.Resources>
</Application>
